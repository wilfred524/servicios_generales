create table Solicitudes(
id_solicitud INT,
codigo_servicio INT,
descripcion_servicio VARCHAR(45),
codigo_tipo INT,
descripcion_tipo VARCHAR(45),
codigo_estado INT,
descripcion_estado VARCHAR(45),
constraint PK_Servicios primary key (id_solicitud)
);

create table Localidades(
id_localidad INT primary key,
codigo_localidad INT,
nombre_localidad VARCHAR(45),
codigo_piso INT,
nombre_piso VARCHAR(45),
codigo_edificio INT,
nombre_edificio VARCHAR(45),
constraint PK_localidad primary key (id_localidad)
);

create table Centros(
id_centro INT primary key,
anio_fiscal INT,
id INT,
descripcion VARCHAR(45),
constraint PK_centro primary key (id_centro)
);

create table fecha(
id_fecha INT primary key,
Anio INT,
mes INT,
dia INT,
constraint PK_fecha primary key (id_fecha)
);
